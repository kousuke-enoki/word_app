APP_ENV=development
APP_PORT=8080
GIN_MODE=debug

DB_HOST=db
DB_USER=postgres
DB_PASSWORD=CHANGE_ME_DB_PASSWORD
DB_NAME=db

JWT_SECRET=CHANGE_ME_JWT_SECRET
CORS_ORIGIN=http://localhost:3000
JWT_EXPIRATION_HOURS="1"
