LOG_LEVEL=debug
ENABLE_REQUEST_LOGGING=true
ENABLE_CLOUDWATCH=false
AWS_REGION=ap-northeast-1
AWS_ACCESS_KEY_ID=CHANGE_ME_AWS_ACCESS_KEY_ID
